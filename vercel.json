{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/docs",
      "dest": "/index.html"
    },
    {
      "src": "/",
      "dest": "/home.html"
    }
  ],
  "rewrites": [
    { "source": "/", "destination": "/" }
  ]
}
